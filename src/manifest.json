{
  "package": "com.tiangouriji.package",
  "name": "一同舔狗日记",
  "versionName": "1.0.9",
  "versionCode": "9",
  "minPlatformVersion": "1060",
  "icon": "/Common/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.clipboard"
    },
    {
      "name": "service.ad"
    },
    {
      "name": "system.device"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "off",
    "designWidth": 1080
  },
  "router": {
    "entry": "Home",
    "pages": {
      "Home": {
        "component": "index"
      },
      "Collect": {
        "component": "index"
      },
      "Detail": {
        "component": "index"
      }
    },
    "widgets": {
      "Card": {
        "name": "舔狗日记",
        "description": "舔狗日记",
        "component": "home",
        "path": "/Card",
        "features": [
          {
            "name": "system.router"
          },
          {
              "name": "system.fetch"
          },
          {
              "name": "system.prompt"
          },
          {
              "name": "system.network"
          },
          {
              "name": "system.storage"
          }
        ],
        "minPlatformVersion": 1060,
        "targetManufactorys": [
          "vivo"
        ],
        "params": {
          "title": "noTitle",
          "height": "33.3%",
          "enableFold": false
        }
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "Home": {
        "titleBarText": "舔狗日记",
        "menu": false,
        "titleBar": false
      },
      "Collect": {
        "titleBarText": "我的收藏",
        "menu": false,
        "titleBar": false
      },
      "Detail": {
        "titleBarText": "详情",
        "menu": false,
        "titleBar": false
      }
    }
  }
}
